<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity DatosUsuario.
    -->
    <changeSet id="20190725111554-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="provincias_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_provincias_id"
                                 referencedColumnNames="id"
                                 referencedTableName="provincias"/>

        <addForeignKeyConstraint baseColumnNames="municipios_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_municipios_id"
                                 referencedColumnNames="id"
                                 referencedTableName="municipios"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="repregcompra_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_repregcompra_id"
                                 referencedColumnNames="id"
                                 referencedTableName="repregcompra"/>

        <addForeignKeyConstraint baseColumnNames="f_pago_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_f_pago_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fpago"/>

        <addForeignKeyConstraint baseColumnNames="zonas_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_zonas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="zonas"/>

        <addForeignKeyConstraint baseColumnNames="transportista_tabla_id"
                                 baseTableName="datos_usuario"
                                 constraintName="fk_datos_usuario_transportista_tabla_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transportista_tabla"/>

    </changeSet>
</databaseChangeLog>
